pipeline {
    agent any
    
    stages {
        stage('Hello') {
            steps {
                echo 'Hello, Jenkins!'
            }
        }
        
        stage('Check') {
            steps {
                echo 'This is a test pipeline.'
            }
        }
        
        stage('Goodbye') {
            steps {
                echo 'Goodbye, Jenkins!'
            }
        }
    }
}

